<?xml version="1.0" encoding="utf-8"?>
<graphml xmlns="http://graphml.graphdrawing.org/xmlns" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://graphml.graphdrawing.org/xmlns http://graphml.graphdrawing.org/xmlns/1.0/graphml.xsd">
  <key attr.name="label" attr.type="string" for="node" id="d1" />
  <key attr.name="label" attr.type="string" for="edge" id="d2" />
  <graph edgedefault="undirected">
    <node id="0">
      <data key="d1">N0</data>
    </node>
    <node id="1">
      <data key="d1">N1</data>
    </node>
    <node id="2">
      <data key="d1">N2</data>
    </node>
    <node id="3">
      <data key="d1">N3</data>
    </node>
    <edge source="0" target="1">
      <data key="d2">N0toN1</data>
    </edge>
<!--
    <edge source="0" target="2">
      <data key="d2">N0toN2</data>
    </edge>
-->
    <edge source="0" target="3">
      <data key="d2">N0toN3</data>
    </edge>
<!--    
    <edge source="1" target="2">
      <data key="d2">N1toN2</data>
    </edge>
-->
    <edge source="1" target="3">
      <data key="d2">N1toN3</data>
    </edge>
  </graph>
</graphml>
